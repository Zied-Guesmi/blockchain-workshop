  "source": "pragma solidity ^0.4.18;\n\n\ncontract Asset {\n\n    address public owner;\n    \n    event changeOwnerEvent(address oldOwner, address newOwner);\n\n    constructor() public {\n        owner = msg.sender;\n    }\n\n    function changeOwner(address newOwner) public {\n        require(owner == msg.sender);\n\n        address oldOwner = owner;\n        owner = newOwner;\n\n        emit changeOwnerEvent(oldOwner, newOwner);\n    }\n\n    /**\n    * The withdraw function, following the withdraw pattern shown and explained here:\n    * http://solidity.readthedocs.io/en/develop/common-patterns.html#withdrawal-from-contracts\n    */\n    // function withdraw() public {\n    //     require(gameFinished && theWinner == msg.sender);\n\n    //     uint amount = gains;\n\n    //     gains = 0;\n    //     msg.sender.transfer(amount);\n    // }\n}",
